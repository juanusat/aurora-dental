	<queryString>
		<![CDATA[select t.trabajador_id ,p.nombre , p.apellido, count(t.trabajador_id)
from cita c
join trabajador t on c.medico_id = t.trabajador_id
join persona p on t.persona_id = p.persona_id
join cargo ca on t.cargo_id = ca.cargo_id
where ca.cargo_id = 1
group by t.trabajador_id , p.nombre, p.apellido
order by t.trabajador_id
limit $P{parLimit};]]>
	</queryString>
