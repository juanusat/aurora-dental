	<queryString>
		<![CDATA[select
    c.medico_id, p.nombre, p.apellido, t.especialidad,
	EXTRACT(YEAR FROM c.fecha_hora) AS anio,
	EXTRACT(WEEK FROM c.fecha_hora) AS numero_semana,
	SUM(c.duracion) as horas_trabajadas
from cita c
JOIN tratamiento tr ON c.tratamiento_id = tr.tratamiento_id
JOIN trabajador t ON c.medico_id = t.trabajador_id
JOIN persona p ON t.persona_id = p.persona_id
WHERE c.estado = 'realizada'
GROUP BY c.medico_id, p.nombre, p.apellido, numero_semana, anio,t.especialidad
ORDER BY c.medico_id;]]>
	</queryString>
